class Solution {
public:
    static vector<int> twoSum(vector<int>& nums, int target) {
        int i = 0;
        int j = 0;
        
        for (i = 0;i< nums.size()-1; ++i)
        {
            //cout << i << endl;
            for (j = i + 1;j < nums.size()-1; ++j)
            {
                
                //cout << j << endl;
                if (nums[i] + nums[j] == target)
                    break;

            }
            if (nums[i] + nums[j] == target)
            {
                break;
            }
        }
        vector<int> ans(2);
        ans[0] = i;
        ans[1] = j;

        return ans;

    }
};
